		"gulp-filter": "^7.0.0",
		"gulp-sourcemaps": "^3.0.0",
		"gulp-typescript": "^6.0.0-alpha.1",
		"gulp-concat": "^2.6.1",
		"node-loader": "^0.6.0",
		"ts-loader": "^8.0.18",
		"typemoq": "^2.1.0",
		"typescript": "^4.4.3",
		"vsce": "^1.97.0",
		"vscode-nls-dev": "^3.2.6",
		"vscode-test": "^1.4.1",
		"webpack": "^5.72.0",
