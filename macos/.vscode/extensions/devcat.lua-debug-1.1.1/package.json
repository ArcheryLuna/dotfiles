{
	"name": "lua-debug",
	"displayName": "Lua Debugger",
	"version": "1.1.1",
	"publisher": "devCAT",
	"icon": "images/icon.png",
	"description": "Visual Studio Code debugger extension for Lua 5.1, 5.2, 5.3",
	"categories": [
		"Debuggers"
	],
	"author": {
		"name": "lee-seungjae"
	},
	"license": "MIT",
	"private": true,
	"scripts": {},
	"engines": {
		"vscode": "^1.0.0"
	},
	"dependencies": {},
	"repository": {
		"type": "git",
		"url": "https://github.com/devcat-studio/VSCodeLuaDebug.git"
	},
	"devDependencies": {},
	"contributes": {
		"debuggers": [
			{
				"type": "lua",
				"label": "Lua Debugger",
				"enableBreakpointsFor": {
					"languageIds": [
						"lua"
					]
				},
				"program": "./DebugAdapter.exe",
				"osx": {
					"runtime": "mono"
				},
				"linux": {
					"runtime": "mono"
				},
				"initialConfigurations": [
					{
						"name": "launch-lua",
						"type": "lua",
						"request": "launch",
						"workingDirectory": "${workspaceRoot}",
						"sourceBasePath": "${workspaceRoot}",
						"executable": "${workspaceRoot}/lua.exe",
						"arguments": "main.lua",
						"listenPublicly": false,
						"listenPort": 56789,
						"encoding": "UTF-8",
						"env": {}
					},
					{
						"name": "launch-gideros",
						"type": "lua",
						"request": "launch",
						"workingDirectory": "${workspaceRoot}",
						"giderosPath": "C:/Program Files (x86)/Gideros",
						"gprojPath": "${workspaceRoot}/GPROJ.gproj",
						"jumpToGiderosErrorPosition": false,
						"stopGiderosWhenDebuggerStops": true,
						"listenPublicly": false,
						"listenPort": 56789,
						"encoding": "UTF-8"
					},
					{
						"name": "wait",
						"type": "lua",
						"request": "attach",
						"workingDirectory": "${workspaceRoot}",
						"sourceBasePath": "${workspaceRoot}",
						"listenPublicly": false,
						"listenPort": 56789,
						"encoding": "UTF-8"
					}
				],
				"configurationAttributes": {
					"launch": {
						"required": [
							"name",
							"type",
							"request",
							"workingDirectory",
							"listenPublicly",
							"listenPort"
						],
						"properties": {
							"workingDirectory": {
								"type": "string",
								"description": "Working directory",
								"default": "${workspaceRoot}"
							},
							"sourceBasePath": {
								"type": "string",
								"description": "Base path of lua source files",
								"default": "${workspaceRoot}"
							},
							"executable": {
								"type": "string",
								"description": "An exe file to launch",
								"default": "${workspaceRoot}/lua.exe"
							},
							"arguments": {
								"type": "string",
								"description": "Command line arguments for executable",
								"default": "main.lua"
							},
							"env": {
								"type": "object",
								"description": "Environment variables for executable",
								"default": {}
							},
							"listenPublicly": {
								"type": "boolean",
								"description": "Set this to false",
								"default": false
							},
							"listenPort": {
								"type": "integer",
								"description": "Local TCP port to communicate between debug adapter and debuggee",
								"default": 56789
							},
							"giderosPath": {
								"type": "string",
								"description": "Gideros installation directory",
								"default": "c:/program files (x86)/gideros"
							},
							"gprojPath": {
								"type": "string",
								"description": "Path of .gproj file",
								"default": "${workspaceRoot}/GPROJ.gproj"
							},
							"encoding": {
								"type": "string",
								"description": "Encoding of the debuggee. (Example: 'UTF-8', '949')",
								"default": "UTF-8"
							},
							"jumpToGiderosErrorPosition": {
								"type": "boolean",
								"description": "Sets whether to jump directly to the source file if it encounters a Gideros error message.",
								"default": false
							},
							"stopGiderosWhenDebuggerStops": {
								"type": "boolean",
								"description": "Stops Gideros Player when debug adapter stops.",
								"default": false
							}
						}
					},
					"attach": {
						"required": [
							"name",
							"type",
							"request",
							"workingDirectory",
							"listenPublicly",
							"listenPort"
						],
						"properties": {
							"workingDirectory": {
								"type": "string",
								"description": "Working directory",
								"default": "${workspaceRoot}"
							},
							"sourceBasePath": {
								"type": "string",
								"description": "Base path of lua source files",
								"default": "${workspaceRoot}"
							},
							"listenPublicly": {
								"type": "boolean",
								"description": "Set this to true if the debuggee will run in different computer.",
								"default": false
							},
							"listenPort": {
								"type": "integer",
								"description": "Local TCP port to communicate between debug adapter and debuggee.",
								"default": 56789
							},
							"encoding": {
								"type": "string",
								"description": "Encoding of the debuggee. (Example: 'UTF-8', '949')",
								"default": "UTF-8"
							}
						}
					}
				}
			}
		]
	},
	"__metadata": {
		"id": "b87f2c47-3ca7-4670-96c2-aacbfc29418f",
		"publisherId": "2542512f-25a6-445f-b93c-efd48a98a7d7",
		"publisherDisplayName": "devCAT",
		"targetPlatform": "undefined",
		"updated": false,
		"isPreReleaseVersion": false,
		"hasPreReleaseVersion": false,
		"installedTimestamp": 1707623033073
	}
}