	"repository": {
		"type": "git",
		"url": "https://github.com/NeverCL/vscode-java-run.git"
	},
	"activationEvents": [
		"onCommand:java.run"
	],
	"main": "./out/src/extension",
	"contributes": {
		"commands": [
			{
				"command": "java.run",
				"title": "Java.Run"
			}
		],
		"keybindings": [
			{
				"command": "java.run",
				"key": "alt+b",
				"mac": "alt+b",
				"when": "editorTextFocus"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "tsc -p ./",
		"compile": "tsc -watch -p ./",
		"postinstall": "node ./node_modules/vscode/bin/install",
		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"typescript": "^2.0.3",
